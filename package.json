{
  "name": "fsm-viewer",
  "displayName": "Finite state machine viewer",
  "description": "View finite state machines using fsm-as-promised library as UML diagrams in VSCode",
  "version": "1.0.0",
  "publisher": "vstirbu",
  "engines": {
    "vscode": "^1.19.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:fsm-viewer.view"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "fsm-viewer.view",
        "title": "View FSM as UML diagram"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.6",
    "eslint": "^4.6.1",
    "@types/node": "^7.0.43",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "d3": "^4.12.2",
    "dagre-d3": "^0.5.4",
    "file-url": "^2.0.2",
    "fsm2dot": "^1.2.0"
  }
}
