{
  "name": "fsm-viewer",
  "displayName": "Finite state machine viewer",
  "description": "View finite state machines using fsm-as-promised library as UML diagrams in VSCode",
  "version": "1.1.0",
  "license": "MIT",
  "publisher": "vstirbu",
  "repository": {
    "type": "git",
    "url": "https://github.com/vstirbu/fsm-viewer.git"
  },
  "engines": {
    "vscode": "^1.19.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:fsm-viewer.view"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "fsm-viewer.view",
        "title": "View FSM as UML diagram"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.43",
    "eslint": "^4.15.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.6"
  },
  "dependencies": {
    "d3": "^4.12.2",
    "dagre-d3": "^0.5.4",
    "file-url": "^2.0.2",
    "fsm2dot": "^1.3.0"
  }
}
